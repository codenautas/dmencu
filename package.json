{
  "name": "dmencu",
  "description": "DM Encu - Encuestas en dispositivo móvil",
  "version": "0.1.6",
  "private": false,
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "main": "dist/server/server/server-dmencu.js",
  "types": "dist/server/server/app-dmencu.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@material-ui/core": "~4.12.3",
    "backend-plus": "^1.16.2",
    "memoize-one": "^6.0.0",
    "meta-enc": "^1.4.3",
    "procesamiento": "^1.0.9",
    "qrcode": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-typed-reducer": "~0.1.1",
    "service-worker-admin": "^0.2.1",
    "yazl": "~2.5.1",
    "cast-error": "^0.1.0",
    "row-validator": "^1.0.0-rc1"
  },
  "dependencias provisorias: las que se pongan en dependencias ponerlas acá y sacarlas juntas": {},
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.10",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/yazl": "~2.4.2",
    "cross-var": "^1.1.0",
    "discrepances": "~0.2.6",
    "mixin-patch": "~0.4.0",
    "pg-triggers": "~0.3.2",
    "types.d.ts": "~0.6.6",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "(tsc -p src/unlogged || echo \"continue w/error\") && (tsc -p src/client || echo \"continue w/error\") && (tsc -p src/server || echo \"continue w/error\") && mixin-patch",
    "build-client": "(tsc -p src/client || echo \"continue w/error\") && mixin-patch",
    "build-unlogged": "(tsc -p src/unlogged || echo \"continue w/error\") && mixin-patch",
    "mixin-patch": "mixin-patch",
    "watch:buildC": "(tsc -p src/client --watch || echo \"continue w/error\")",
    "watch:buildS": "(tsc -p src/server --watch || echo \"continue w/error\")",
    "watch:buildU": "(tsc -p src/unlogged --watch || echo \"continue w/error\")",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "start": "node dist/server/server/server-dmencu.js",
    "dump": "npm start -- --dump-db",
    "bdump": "npm run build && npm run dump"
  },
  "config": {
    "stackFolder": "../../codenautas"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "app",
    "ecmaVersion": 6
  },
  "mixin-patch": {
    "patch": false,
    "copy": [
      {
        "from": "src/client",
        "to": "dist/client/client"
      },
      {
        "from": "src/unlogged",
        "to": "dist/unlogged/unlogged"
      },
      {
        "from": "src/unlogged",
        "to": "dist/client/unlogged"
      }
    ]
  }
}
