{
  "name": "dmencu",
  "description": "DM Encu - Encuestas en dispositivo móvil",
  "version": "0.1.0",
  "private": true,
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "main": "dist/server/server-dmencu.js",
  "types": "dist/server/app-dmencu.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@material-ui/core": "~4.11.2",
    "memoize-one": "^5.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-typed-reducer": "~0.1.1",
    "yazl": "~2.5.1",
    "ajax-best-promise": "^0.3.7",
    "backend-plus": "^1.7.4",
    "best-globals": "^0.10.31",
    "operativos": "^1.1.6",
    "rel-enc": "^1.2.4",
    "meta-enc": "^1.2.2",
    "procesamiento": "^1.0.2",
    "row-validator": "^0.6.1",
    "typed-controls": "^0.8.25",
    "qrcode": "^1.4.4"
  },
  "dependencias provisorias: las que se pongan en dependencias ponerlas acá y sacarlas juntas": {},
  "devDependencies": {
    "@types/qrcode": "^1.3.5",
    "@types/node": "^14.14.22",
    "@types/mocha": "^8.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/yazl": "~2.4.2",
    "discrepances": "~0.2.4",
    "pg-triggers": "~0.2.3",
    "types.d.ts": "~0.5.2",
    "typescript": "^4.1.3",
    "mixin-patch": "~0.3.3",
    "cross-var": "^1.1.0"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "(tsc -p src/unlogged || echo \"continue w/error\") && (tsc -p src/client || echo \"continue w/error\") && (tsc -p src/server || echo \"continue w/error\") && mixin-patch",
    "build-unlogged": "(tsc -p src/unlogged || echo \"continue w/error\") && mixin-patch",
    "mixin-patch": "mixin-patch",
    "watch:buildC": "(tsc -p src/client --watch || echo \"continue w/error\")",
    "watch:buildS": "(tsc -p src/server --watch || echo \"continue w/error\")",
    "watch:buildU": "(tsc -p src/unlogged --watch || echo \"continue w/error\")",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "start": "node dist/server/server-dmencu.js",
    "dump": "npm start -- --dump-db",
    "bdump": "npm run build && npm run dump"
  },
  "config": {
    "stackFolder": "../../codenautas"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "app",
    "ecmaVersion": 6
  }
}
